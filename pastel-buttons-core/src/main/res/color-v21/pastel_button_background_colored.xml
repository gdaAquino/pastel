<?xml version="1.0" encoding="utf-8"?>

<!-- ColorStateList for L and above Colored Button.
     For pre M it will be use as the backgroundTint for the Button
     while M and above it will be use as the tint on the Drawable Resource. -->

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:alpha="?android:attr/disabledAlpha"
        android:color="?colorButtonNormal"
        android:state_enabled="false"/>
    <item
        android:color="?colorAccent"/>
</selector>